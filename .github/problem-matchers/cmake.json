{
  "problemMatcher": [
    {
      "owner": "cmake-error",
      "severity": "error",
      "pattern": [
        {
          "regexp": "^CMake (Error|FATAL_ERROR)\\s+at\\s+(.+?):(\\d+)\\s+\\((.+?)\\):(.*)$",
          "severity": 1,
          "file": 2,
          "line": 3,
          "message": 5
        }
      ]
    },
    {
      "owner": "cmake-warning",
      "severity": "warning",
      "pattern": [
        {
          "regexp": "^CMake (Warning)\\s+at\\s+(.+?):(\\d+)\\s+\\((.+?)\\):(.*)$",
          "severity": 1,
          "file": 2,
          "line": 3,
          "message": 5
        }
      ]
    },
    {
      "owner": "cmake-general",
      "severity": "error",
      "pattern": [
        {
          "regexp": "^CMake Error:\\s+(.*)$",
          "message": 1
        }
      ]
    }
  ]
}
